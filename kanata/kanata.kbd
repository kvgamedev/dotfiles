(defcfg
	process-unmapped-keys yes
)

(defsrc
	caps
	s y u i o p h j k l ;
)

(defalias
	caps (tap-hold 70 70 esc lctl)
	;_super (tap-hold 200 200 ; lmet)
	s_symbols (tap-hold 200 200 s (layer-toggle symbols))
)

(deflayer default
	@caps
	@s_symbols _ _ _ _ _ _ _ _ _ @;_super
)

(deflayer symbols
	_
	_ S-6 S-7 S-8 S-9 S-0 [ ] { } \
)	
